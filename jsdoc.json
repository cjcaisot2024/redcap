{
  "plugins": ["plugins/markdown"],
  "markdown": {
    "tags": ["foo", "bar"]
  },
  "source": {
    "include": ["assets"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(node_modules|\\.min\\.js$)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "recurse": "true",
    "destination": "./documents/documentation/jsDocs",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark"
    }
  }
}
