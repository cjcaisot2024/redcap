<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org">

    <paths>
        <directories>
            <directory>./</directory> <!-- Ajusta este valor según la estructura de tu proyecto -->
        </directories>
    </paths>
    <output>
        <directory>./documents/documentation/phpDocs</directory> <!-- Directorio donde se generará la documentación -->
    </output>
</phpdocumentor>
